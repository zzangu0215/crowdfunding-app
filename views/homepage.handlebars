{{#each projects as |project|}}
<div class='row align-center mb-5'>
    <div class='col-md-6'>
        <h2>{{project.name}}</h2>
    </div>

    <div class='col-md-6'>
        <h2>{{project.description}}</h2>
    </div>

    <div class='col-md-6'>
        <h2>{{project.needed_funding}}</h2>
    </div>
</div>
{{/each}}